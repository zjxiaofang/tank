<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.tank.mapper.MyMapper">
    <insert id="saveInformation" parameterType="com.tank.domain.BasicInformation">
        insert (gmt_create,gmt_modified,username,birthday,) into basic_information values(#{id},#{gmtCreate},#{gmtModified},#{username},#{birthday},#{sex},#{address},#{phoneNumber},#{mail},#{identity},#{workTime},#{introduce},#{userID});
    </insert>
</mapper>
